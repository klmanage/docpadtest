---
title: Articles
layout: article
isPage: true
---

<ul class="list-unstyled">
	<% for document in @getCollection('articles').toJSON(): %>
		<li>
			<div><%= document.date.toDateString() %></div>
			<h3><%= document.title %></h3>
			<div><%=document.content %></div>
		</li>
	<% end %>
</ul>